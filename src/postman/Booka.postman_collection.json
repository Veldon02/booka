			"event": [
				{
					"listen": "prerequest",
					"script": {
						"type": "text/javascript",
						"packages": {},
						"exec": [
							"var endpoint = pm.collectionVariables.get('BackofficeBaseUrl') + '/auth/login'\r",
							"\r",
							"var requestConfig = {\r",
							"    method: 'POST',\r",
							"    url: endpoint,\r",
							"    header: 'Content-Type: application/json',\r",
							"    body: {\r",
							"        mode: 'raw',\r",
							"        raw: JSON.stringify({\r",
							"            \"email\": \"string\",\r",
							"            \"password\": \"string\"\r",
							"        })\r",
							"    }\r",
							"};\r",
							"\r",
							"pm.sendRequest(requestConfig, function (err, response) {\r",
							"    if (err) {\r",
							"        console.error(err);\r",
							"    } else {\r",
							"        if (response.code === 200) {\r",
							"            var responseBody = response.json();\r",
							"            var token = responseBody.token;\r",
							"            pm.collectionVariables.set('BackofficeToken', token);\r",
							"        } else {\r",
							"            console.error('Failed to login. Response code:', response.code);\r",
							"        }\r",
							"    }\r",
							"});"
						]
					}
				},
